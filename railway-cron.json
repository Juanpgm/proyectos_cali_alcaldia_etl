{
    "services": [
        {
            "name": "etl-cron-job",
            "source": {
                "type": "github",
                "repo": "Juanpgm/proyectos_cali_alcaldia_etl",
                "branch": "main"
            },
            "schedule": "0 2 * * *",
            "command": "python -m pip install -r requirements.txt && python load_app/bulk_load_data.py --data-type all",
            "environment": {
                "DATABASE_URL": "${{Postgres.DATABASE_URL}}"
            }
        }
    ]
}