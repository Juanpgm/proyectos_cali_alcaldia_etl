# Variables de entorno para ETL System
# Basado en las variables del archivo .env principal

# Configuración PostgreSQL Local
DB_HOST=localhost
DB_PORT=5432
DB_NAME=dev
DB_USER=postgres
DB_PASSWORD=root
DB_SCHEMA=public

# Configuración de Pool de Conexiones
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_TIMEOUT=30

# Configuración PostGIS (datos geoespaciales)
ENABLE_POSTGIS=true
POSTGIS_VERSION=3.3

# Variables alternativas (compatibilidad)
POSTGRES_SERVER=localhost
POSTGRES_PORT=5432
POSTGRES_DB=dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=root