# Cloud Function Deployment Configuration
# Para deploy: gcloud functions deploy load-unidades-proyecto --gen2 ...

# Variables de entorno requeridas
S3_BUCKET_NAME=unidades-proyecto-documents
AWS_CREDENTIALS_SECRET=projects/YOUR_PROJECT_ID/secrets/aws-credentials
GCP_PROJECT_ID=YOUR_PROJECT_ID
REGION=us-central1

# Recursos
MEMORY=512MB
TIMEOUT=540
MAX_INSTANCES=1
MIN_INSTANCES=0

# Service Account
SERVICE_ACCOUNT=cloud-functions-sa@YOUR_PROJECT_ID.iam.gserviceaccount.com
